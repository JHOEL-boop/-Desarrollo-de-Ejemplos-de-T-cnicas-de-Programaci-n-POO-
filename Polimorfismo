class Figura:
    def dibujar(self):
        print("Dibujando figura")

class Circulo(Figura):
    def dibujar(self):
        print("Dibujando c√≠rculo")

class Cuadrado(Figura):
    def dibujar(self):
        print("Dibujando cuadrado")

# Uso
f1 = Circulo()
f2 = Cuadrado()
f1.dibujar()
f2.dibujar()
